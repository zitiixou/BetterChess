{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Chess.com Chess Aid",
  "description": "Chess.com extension to highlight the best move and key squares, as well as show the evaluation bar",
  "version": "3.2.3",
  "manifest_version": 3,
  "icons": {
    "16": "img/favicon/knight-16.png",
    "48": "img/favicon/knight-48.png",
    "128": "img/favicon/knight-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://www.chess.com/*"],
      "js": ["js/chess.min.js"]
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "host_permissions": [
    "https://www.chess.com/*"
],
"key": ""
}
